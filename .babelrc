{
  "env": {
    "development": {
      "presets": [
        "env",
        "react",
	      "stage-3",
        "flow",
      ],
      "plugins": [
        "syntax-dynamic-import",
      ],
    },
    "production": {
      "presets": [
        "env",
        "react",
        "stage-3",
        "flow",
      ],
      "plugins": [
        "syntax-dynamic-import",
        "remove-test-ids",
        "transform-react-constant-elements",
        "transform-react-remove-prop-types",
      ],
    },
    "test": {
      "presets": [
        "env",
        "react",
        "stage-3",
        "flow",
      ],
      "plugins": [
        "syntax-dynamic-import",
      ],
    },
  },
}
